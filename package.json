{
    "name": "schulkalender",
    "version": "0.1.2",
    "private": true,
    "scripts": {
        "server": "cd server && npm run dev",
        "client": "cd client && npm run dev",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "cd client && npm run build && cd ../server && npm install",
        "update": "node server/scripts/update.js"
    },
    "dependencies": {
        "concurrently": "^8.2.2"
    }
}